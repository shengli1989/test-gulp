doctype html
html(lang='zh-TW')
  head
    block head
      include ./shared/_head
    block title
      title= site.title
    block keywords
      meta(name='keywords' content=site.keywords)
    block description
      meta(name='description' content=site.description)
    block og
      include ./shared/_og
  body
    .page
      block content
    block foot
      include ./shared/_foot
